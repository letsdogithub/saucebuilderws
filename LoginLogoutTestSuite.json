{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://demo.actitime.com/login.do"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "username"
      },
      "text": "admin"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "pwd"
      },
      "text": "manager"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Login"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Enter Time-Track"
    },
    {
      "type": "verifyTextPresent",
      "text": "Robert Wooster"
    },
    {
      "type": "verifyTextPresent",
      "text": "Logout"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "logoutLink"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Login"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}